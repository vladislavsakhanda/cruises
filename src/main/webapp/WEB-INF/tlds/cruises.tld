<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
        version="2.0">

    <tlib-version>2.0</tlib-version>
    <short-name>tlds</short-name>
    <uri>/WEB-INF/tlds/cruises.tld</uri>
    <tag>
        <name>CruisesTLD</name>
        <tag-class>com.my.tlds.CruisesTLD</tag-class>
        <body-content>empty</body-content>
    </tag>

    <function>
        <name>countPriceLiner</name>
        <function-class>com.my.tlds.CruisesTLD</function-class>
        <function-signature>double countPriceLiner(db.dao.mysql.entity.Liner)</function-signature>
    </function>

    <function>
        <name>getLinerById</name>
        <function-class>com.my.tlds.CruisesTLD</function-class>
        <function-signature>db.dao.mysql.entity.Liner getLinerById(long)</function-signature>
    </function>

    <function>
        <name>getLinerFreePlaces</name>
        <function-class>com.my.tlds.CruisesTLD</function-class>
        <function-signature>int getLinerFreePlaces(db.dao.mysql.entity.Liner)</function-signature>
    </function>

    <function>
        <name>getLinerRouteInMap</name>
        <function-class>com.my.tlds.CruisesTLD</function-class>
        <function-signature>java.util.HashMap getLinerRouteInMap(db.dao.mysql.entity.Liner)</function-signature>
    </function>

    <function>
        <name>getAllTripByUserId</name>
        <function-class>com.my.tlds.CruisesTLD</function-class>
        <function-signature>java.util.List getAllTripByUserId(long)</function-signature>
    </function>

    <function>
        <name>getAllTrip</name>
        <function-class>com.my.tlds.CruisesTLD</function-class>
        <function-signature>java.util.List getAllTrip()</function-signature>
    </function>

    <function>
        <name>getTripByUserIdAndLinerId</name>
        <function-class>com.my.tlds.CruisesTLD</function-class>
        <function-signature>db.dao.mysql.entity.Trip getTripByUserIdAndLinerId(long, long)</function-signature>
    </function>
</taglib>